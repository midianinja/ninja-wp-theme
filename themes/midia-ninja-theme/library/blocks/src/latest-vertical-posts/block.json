{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ninja/latest-vertical-posts",
	"version": "0.1.10",
	"title": "Últimos posts (Vertical)",
	"category": "text",
	"icon": "excerpt-view",
	"description": "Exibe os últimos posts em formato vertical e com slider.",
	"example": {},
	"supports": {
		"html": false
	},
	"attributes": {
		"blockId": {
			"type": "string"
		},
		"blockModel": {
			"type": "string",
			"default": "posts"
		},
		"columns": {
			"type": "number",
			"default": 2
		},
		"flickrAlbumId": {
			"type": "string"
		},
		"flickrAPIKey": {
			"type": "string"
		},
		"flickrByType": {
			"type": "string",
			"default": "user"
		},
		"flickrUserId": {
			"type": "string"
		},
		"gridFormat": {
			"type": "string",
			"default": "columns"
		},
		"heading": {
			"type": "string"
		},
		"order": {
			"type": "string",
			"default": "desc"
		},
		"orderBy": {
			"type": "string",
			"default": "date"
		},
		"playlistId": {
			"type": "string"
		},
		"postsBySlide": {
			"type": "number",
			"default": 3
		},
		"postsToShow": {
			"type": "number",
			"default": 8
		},
		"postType": {
			"type": "string",
			"default": "post"
		},
		"queryTerms": {
			"type": "array"
		},
		"showAsGrid": {
			"type": "boolean",
			"default": false
		},
		"showAuthor": {
			"type": "boolean",
			"default": true
		},
		"showChildren": {
			"type": "boolean",
			"default": false
		},
		"showDate": {
			"type": "boolean",
			"default": false
		},
		"showExcerpt": {
			"type": "boolean",
			"default": false
		},
		"showThumbnail": {
			"type": "boolean",
			"default": true
		},
		"showTaxonomy": {
			"type": "string",
			"default": "category"
		},
		"taxonomy": {
			"type": "string"
		},
		"thumbnailFormat": {
			"type": "boolean",
			"default": false
		}
	},
	"textdomain": "ninja",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"viewScript": "file:./view.js"
}
