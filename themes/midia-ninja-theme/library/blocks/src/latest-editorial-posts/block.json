{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "ninja/latest-editorial-posts",
	"version": "0.1.0",
	"title": "Últimos posts (Editorias)",
	"category": "text",
	"icon": "excerpt-view",
	"description": "Exibe os últimos posts de Editorias com filtros.",
	"example": {},
	"supports": {
		"html": false
	},
    "attributes": {
        "blockId": {
            "type": "string"
        },
        "order": {
            "type": "string",
            "default": "desc"
        },
        "orderBy": {
            "type": "string",
            "default": "date"
        },
        "postsToShow": {
            "type": "number",
            "default": 5
        }
	},
	"textdomain": "ninja",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
